{
  "name": "devrahimi-tanstack-router-template",
  "private": true,
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": "22.x",
    "bun": "please-use-pnpm",
    "npm": "please-use-pnpm",
    "pnpm": "10.x"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "prepare": "husky",
    "format": "prettier . --write --config-precedence prefer-file .prettierrc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run format",
      "pnpm run lint"
    ]
  },
  "dependencies": {
    "@tanstack/react-router": "^1.121.2",
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/eslint-plugin-router": "^1.120.17",
    "@tanstack/react-router-devtools": "^1.121.12",
    "@tanstack/router-plugin": "^1.121.13",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.0.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.1.0"
  },
  "overrides": {
    "@tanstack/react-router": "^1.121.2"
  }
}
